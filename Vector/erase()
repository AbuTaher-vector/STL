#include<bits/stdc++.h>
using namespace std;
#define nn int
#define dd double
#define ll long long int
#define ss string
#define nl '\n'
int main()
{
    /*erase() function is used to remove elements
     from a container from the specified position or range.
    */
    vector<nn>v{1,2,3,4,5,6,7};
    v.erase(v.begin()+2);
    for(nn i=0;i<v.size();i++) cout<<v[i]<<" ";
        cout<<nl;
}
