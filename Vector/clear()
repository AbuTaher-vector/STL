#include<bits/stdc++.h>
using namespace std;
#define nn int
#define dd double
#define ll long long int
#define ss string
#define nl '\n'
int main()
{
    /*clear() function is used to remove all the elements of
     the vector container, thus making it size 0. 
    */
    vector<nn>v{1,2,3,4,5,6,7};
    v.clear();
    cout<<v.size()<<nl;
    
}
