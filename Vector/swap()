#include<bits/stdc++.h>
using namespace std;
#define nn int
#define dd double
#define ll long long int
#define ss string
#define nl '\n'
int main()
{
    /*swap() function is used to swap the contents of one 
    vector with another vector of same type and sizes of vectors may differ.
    */
    vector<nn>a(5),b(5);
    a={2,3,4,5,6};
    b={7,8,9,10,11};
    a.swap(b);
    for(auto it=a.begin();it!=a.end();it++) cout<<*it<<" ";
        cout<<nl;
        for(auto it=b.begin();it!=b.end();it++) cout<<*it<<" ";
            cout<<nl;
    
}
