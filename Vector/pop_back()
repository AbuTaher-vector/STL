#include<bits/stdc++.h>
using namespace std;
#define nn int
#define dd double
#define ll long long int
#define ss string
#define nl '\n'
int main()
{
    /*pop_back() function is used to pop or remove elements from a vector from the back.
     The value is removed from the vector from the end, and the container size is decreased by 1. 
    */
    vector<nn>v{1,2,3,4,5,6,7};
    v.pop_back();
    for(nn i=0;i<v.size();i++) cout<<v[i]<<" ";
        cout<<nl;
    
}
