#include<bits/stdc++.h>
using namespace std;
#define nn int
#define dd double
#define ll long long int
#define ss string
#define nl '\n'
int main()
{
   /*
   begin() function is used to return an iterator pointing to the first element of the map container.
    begin() function returns a bidirectional iterator to the first element of the container.
   */
     map<char,nn>mp;
     mp['a']=1;
     mp['b']=2;
     mp['c']=3;

     for(auto it=mp.begin();it!=mp.end();it++) cout<<it->first<<" "<<it->second<<nl;


}
